{
  "name": "tic-tac-leg",
  "description": "More than tic tac toe",
  "version": "0.0.1",
  "scripts": {
    "start": "webpack-dev-server --hot --inline --open",
    "lint": "eslint -f codeframe --ext js,jsx src",
    "test": "karma start karma.conf.js",
    "precommit": "lint-staged",
    "prettier": "prettier --single-quote --trailing-comma es5 --tab-width 4 --write \"src/**/*.js{,x}\""
  },
  "author": "Zacaria",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/Zacaria/TicTacLeg.git"
  },
  "bugs": {
    "url": "https://github.com/Zacaria/TicTacLeg/issues"
  },
  "homepage": "https://github.com/Zacaria/TicTacLeg#readme",
  "devDependencies": {
    "babel-core": "^6.24.1",
    "babel-loader": "^7.0.0",
    "babel-polyfill": "^6.23.0",
    "babel-preset-es2015": "^6.24.1",
    "babel-preset-react": "^6.24.1",
    "babel-preset-stage-2": "^6.24.1",
    "chai-spies": "^0.7.1",
    "eslint": "^3.19.0",
    "eslint-config-airbnb": "^14.1.0",
    "eslint-config-airbnb-base": "^11.1.3",
    "eslint-config-prettier": "^2.0.0",
    "eslint-plugin-import": "^2.2.0",
    "eslint-plugin-jsx-a11y": "^4.0.0",
    "eslint-plugin-prettier": "^2.0.1",
    "eslint-plugin-react": "^6.10.3",
    "file-loader": "^0.11.1",
    "karma": "^1.6.0",
    "karma-chrome-launcher": "^2.1.0",
    "karma-mocha": "^1.3.0",
    "karma-phantomjs-launcher": "^1.0.4",
    "karma-sourcemap-loader": "^0.3.7",
    "karma-spec-reporter": "0.0.31",
    "karma-webpack": "^2.0.3",
    "mocha": "^3.3.0",
    "prettier": "^1.3.1",
    "react-addons-test-utils": "^15.5.1",
    "redux-devtools-extension": "^2.13.1",
    "webpack": "^2.5.0",
    "webpack-dev-server": "^2.4.5"
  },
  "dependencies": {
    "chai": "^3.5.0",
    "enzyme": "^2.8.2",
    "prop-types": "^15.5.8",
    "react": "^15.5.4",
    "react-dom": "^15.5.4",
    "react-redux": "^5.0.4",
    "redux": "^3.6.0",
    "redux-logger": "^3.0.1",
    "redux-thunk": "^2.2.0"
  },
  "lint-staged": {
    "*.{js,jsx}": [
      "eslint --fix -f codeframe --ext js,jsx src",
      "git add"
    ],
    "gitDir": "../../../../"
  },
  "eslintConfig": ".eslintrc"
}
